{
  "name": "Easy Going with LinkedIn",
  "version": "1.0.0",
  "description": "LinkedIn tool for screening candidate profiles and exporting data to Excel or your HR system.",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "http://localhost:3000/"
  ],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup-sign-in.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 3
}
